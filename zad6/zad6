query:
db.reviews.mapReduce(
    function() {
        if (this.product && this.review) {
            const product = this.product
            const review = this.review
            
            emit(
                product.productId,
                {
                    price: product.price,
                    title: product.title,
                    review: review
                }
            )
        }
    },
    function(key, values) {
        var title = ''
        var price = null
        const reviews = values.map(v => v.review)

        values.forEach(v => {
            title = v.title
            price = v.price
        })

        if (price) {
            return {
                title,
                price,
                reviews
            }
        } else {
            return {
                title,
                reviews
            }
        }
    },
    {
        out: 'products'
    }
);

db.products.find();

